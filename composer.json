{
    "name": "marianvlad/nova-env-card",
    "description": "A Laravel Nova card for editing .env file .",
    "keywords": [
        "laravel",
        "nova"
    ],
    "homepage": "https://github.com/marianvlad/nova-env-card",
    "license": "MIT",
    "authors": [
        {
            "name": "Marian Gabriel",
            "email": "vladmariang@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "*"
    },
    "autoload": {
        "psr-4": {
            "Marianvlad\\NovaEnvCard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marianvlad\\NovaEnvCard\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
